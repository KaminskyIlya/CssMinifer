2)
body { font-size: 14px; } @media (max-width: 480px) { body {font-size: 12px;} } html { font-size: 14px; } @media (max-width: 480px) { html {font-size: 12px;} }
body,html{font-size:14px}@media (max-width:480px){body,html{font-size:12px}}
body{font-size:14px}@media (max-width: 480px){body{font-size:12px}}html{font-size:14px}@media (max-width: 480px){html{font-size:12px}}
body{font-size:14px}@media(max-width:480px){body{font-size:12px}}html{font-size:14px}@media(max-width:480px){html{font-size:12px}}
body,html{font-size:14px}@media (max-width:480px){body,html{font-size:12px}}

body{font-size:14px;}@media(min-width:480px)and(max-width: 840px){body{font-size:12px;}}html{font-size:14px;}@media(max-width:840px)and(min-width:480px){html{font-size:12px;}}
body,html{font-size:14px}@media (min-width:480px) and (max-width:840px){body{font-size:12px}}@media (max-width:840px) and (min-width:480px){html{font-size:12px}}
body,html{font-size:14px}@media(min-width:480px)and(max-width:840px){body,html{font-size:12px}} - ideal

